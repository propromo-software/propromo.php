  url: {{baseUrl}}/repos/:owner/:repo/subscription
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "subscribed": "<boolean>",
    "ignored": "<boolean>"
  }
}
