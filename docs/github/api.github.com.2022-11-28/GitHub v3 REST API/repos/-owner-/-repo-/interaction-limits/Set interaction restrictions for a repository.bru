meta {
  name: Set interaction restrictions for a repository
  type: http
  seq: 2
}

put {
  url: {{baseUrl}}/repos/:owner/:repo/interaction-limits
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "limit": "contributors_only",
    "expiry": "three_days"
  }
}
