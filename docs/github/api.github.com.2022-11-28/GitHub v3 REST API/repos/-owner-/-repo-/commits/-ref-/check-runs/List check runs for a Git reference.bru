meta {
  name: List check runs for a Git reference
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/repos/:owner/:repo/commits/:ref/check-runs?check_name=<string>&status=in_progress&filter=latest&per_page=30&page=1&app_id=<integer>
  body: none
  auth: none
}

query {
  check_name: <string>
  status: in_progress
  filter: latest
  per_page: 30
  page: 1
  app_id: <integer>
}

headers {
  Accept: application/json
}
