  url: {{baseUrl}}/repos/:owner/:repo/actions/oidc/customization/sub
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "use_default": "<boolean>",
    "include_claim_keys": [
      "<string>",
      "<string>"
    ]
  }
}
