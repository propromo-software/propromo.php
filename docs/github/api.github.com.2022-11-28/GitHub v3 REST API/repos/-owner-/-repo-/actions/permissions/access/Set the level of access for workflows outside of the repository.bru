meta {
  name: Set the level of access for workflows outside of the repository
  type: http
  seq: 2
}

put {
  url: {{baseUrl}}/repos/:owner/:repo/actions/permissions/access
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "access_level": "organization"
  }
}
